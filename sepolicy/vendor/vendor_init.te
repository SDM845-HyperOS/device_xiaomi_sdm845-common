# Allow vendor_init to write to vendor_sysfs_ssr_toggle
allow vendor_init vendor_sysfs_ssr_toggle:file w_file_perms;

allow vendor_init proc_sched:file w_file_perms;
allow vendor_init proc_watermark_scale_factor:file w_file_perms;
allow vendor_init blkio_file:file w_file_perms;

set_prop(vendor_init, vendor_fingerprint_prop)
set_prop(vendor_init, vendor_cnd_prop)

allow vendor_init {
  proc_dirty
  proc
}:file w_file_perms;

allow vendor_init self:perf_event cpu;
allow vendor_init self:capability kill;
